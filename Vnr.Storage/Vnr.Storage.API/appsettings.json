{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=storage.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "StoredFilesPath": "StaticFiles",
  "FileSizeLimitConfiguration": {
    "StreamFileSizeLimit": 204857600,
    "DefaultFileSizeLimit": 2097152,
    "DocxFileSizeLimit": 2097152,
    "ExcelFileSizeLimit": 2097152,
    "PdfFileSizeLimit": 2097152
  },
  "StreamedFileUploadPhysicalPermittedExtensionsConfiguration": {
    "SingleFileUploadPermittedExtensions": [
      ".txt",
      ".pdf",
      ".docx"
    ]
  },
  "BufferedFileUploadPhysicalPermittedExtensionsConfiguration": {
    "SingleFileUploadPermittedExtensions": [
      ".txt",
      ".pdf",
      ".docx"
    ],
    "MultipleFileUploadPermittedExtensions": [
      ".txt",
      ".pdf",
      ".docx"
    ]
  },
  "EncryptedFileExtensionsConfiguration": {
    "EncryptedFileExtensions": [
      ".vnresource"
    ]
  }
}